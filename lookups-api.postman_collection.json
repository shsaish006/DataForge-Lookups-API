{
  "info": {
    "name": "Topcoder Lookups API",
    "description": "Complete API collection for the modernized Topcoder Lookups API with NestJS, TypeScript, Prisma, and PostgreSQL",
    "version": "1.0.0",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "auth": {
    "type": "bearer",
    "bearer": [
      {
        "key": "token",
        "value": "{{jwt_token}}",
        "type": "string"
      }
    ]
  },
  "variable": [
    {
      "key": "base_url",
      "value": "http://localhost:8000",
      "type": "string"
    },
    {
      "key": "api_prefix",
      "value": "/api/v1",
      "type": "string"
    },
    {
      "key": "jwt_token",
      "value": "",
      "type": "string"
    }
  ],
  "item": [
    {
      "name": "Countries",
      "item": [
        {
          "name": "Get All Countries",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}{{api_prefix}}/countries?skip=0&take=20&orderBy=name&orderDirection=asc",
              "host": ["{{base_url}}"],
              "path": ["{{api_prefix}}", "countries"],
              "query": [
                {
                  "key": "skip",
                  "value": "0",
                  "description": "Number of records to skip"
                },
                {
                  "key": "take",
                  "value": "20",
                  "description": "Number of records to take"
                },
                {
                  "key": "search",
                  "value": "",
                  "description": "Search term",
                  "disabled": true
                },
                {
                  "key": "orderBy",
                  "value": "name",
                  "description": "Field to order by"
                },
                {
                  "key": "orderDirection",
                  "value": "asc",
                  "description": "Order direction (asc/desc)"
                }
              ]
            }
          },
          "response": []
        },
        {
          "name": "Get Country by ID",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}{{api_prefix}}/countries/{{country_id}}",
              "host": ["{{base_url}}"],
              "path": ["{{api_prefix}}", "countries", "{{country_id}}"]
            }
          },
          "response": []
        },
        {
